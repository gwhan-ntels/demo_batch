    <insert id="saveIfCustInfoAll" parameterType="Multi">
				INSERT INTO IFNBRM_CUST_INFO (
				  SO_ID, CUST_ID, PYM_ACNT_ID, CUST_NM, ACNT_NM,
				  CUST_TP, ZIP_CD, BASE_ADDR, ADDR_DTL, MTEL_NO,
				  TEL_NO, FAX_NO, EML, BIZ_REG_NO, BUSI_CNDT,
				  BUSI_TP, TAX_TP, PZIP_CD, PBASE_ADDR, PADDR_DTL,
				  CITY, STATE, STATE_NM, PMTEL_NO, PTEL_NO,
				  PFAX_NO, PEML, PYM_MTHD, BILL_MDM_SMS_YN, BILL_MDM_GIRO_YN,
				  BILL_MDM_EML_YN, BNK_CD, CDTCD_EXP_DT, USE_ACNT_NM_YN, ACNT_OWNER_NM,
				  ACNT_NO, TBR_FLG, ARRS_NOBILL_YN, PYM_DT
				)
				SELECT 
				  B.SO_ID,
				  B.CUST_ID,
				  B.PYM_ACNT_ID,
				  B.CUST_NM,
				  B.ACNT_NM,
				  B.CUST_TP,
				  B.ZIP_CD,
				  B.BASE_ADDR,
				  B.ADDR_DTL,
				  B.MTEL_NO,
				  B.TEL_NO,
				  B.FAX_NO,
				  B.EML,
				  B.BIZ_REG_NO,
				  B.BUSI_CNDT,
				  B.BUSI_TP,
				  B.TAX_TP,
				  B.PZIP_CD,
				  B.PBASE_ADDR,
				  B.PADDR_DTL,
				  B.CITY,
				  B.STATE,
				  B.STATE_NM,
				  B.PMTEL_NO,
				  B.PTEL_NO,
				  B.PFAX_NO,
				  B.PEML,
				  B.PYM_MTHD,
				  B.BILL_MDM_SMS_YN,
				  B.BILL_MDM_GIRO_YN,
				  B.BILL_MDM_EML_YN,
				  B.BNK_CD,
				  B.CDTCD_EXP_DT,
				  B.USE_ACNT_NM_YN,
				  B.ACNT_OWNER_NM,
				  B.ACNT_NO,
				  B.TBR_FLG,
				  B.ARRS_NOBILL_YN,
				  B.PYM_DT
				FROM 
				(
				  SELECT 
				    A.SO_ID,
				    A.CUST_ID,
				    B.PYM_ACNT_ID,
				    A.CUST_NM,
				    B.ACNT_NM,
				    CASE WHEN A.CUST_TP = '' then 'Y' ELSE A.CUST_TP  END CUST_TP,
				    A.ZIP_CD,
				    A.BASE_ADDR,
				    A.ADDR_DTL,
				    A.MTEL_NO,
				    A.TEL_NO,
				    A.FAX_NO,
				    A.EML,
				    A.BIZ_REG_NO,
				    A.BUSI_CNDT,
				    A.BUSI_TP,
				    A.TAX_TP,
				    B.ZIP_CD AS PZIP_CD,
				    B.BASE_ADDR AS PBASE_ADDR,
				    B.ADDR_DTL AS PADDR_DTL,
				    B.CITY,
				    B.STATE,
				    NULL AS STATE_NM,
				    B.MTEL_NO AS PMTEL_NO,
				    B.TEL_NO AS PTEL_NO,
				    B.FAX_NO AS PFAX_NO,
				    B.EML AS PEML,
				    B.PYM_MTHD,
				    B.BILL_MDM_SMS_YN,
				    B.BILL_MDM_GIRO_YN,
				    B.BILL_MDM_EML_YN,
				    B.BNK_CD,
				    B.CDTCD_EXP_DT,
				    B.USE_ACNT_NM_YN,
				    B.ACNT_OWNER_NM,
				    B.ACNT_NO,
				    B.TBR_FLG,
				    B.ARRS_NOBILL_YN,
				    B.PYM_DT
				  FROM   (
				    SELECT SO_ID,
				      CUST_ID,
				      PYM_ACNT_ID
				    FROM   TCMCT_CTRT_INFO
				    GROUP BY SO_ID, CUST_ID, PYM_ACNT_ID ) Z,
				      TCMCU_CUST_INFO A,
				      TCMCU_PYM_ACNT_INFO B
				) B
                ON DUPLICATE KEY UPDATE PYM_ACNT_ID           = B.PYM_ACNT_ID      
							 ,CUST_NM              = B.CUST_NM         
							 ,ACNT_NM              = B.ACNT_NM         
							 ,CUST_TP              = B.CUST_TP         
							 ,ZIP_CD               = B.ZIP_CD          
							 ,BASE_ADDR            = B.BASE_ADDR       
							 ,ADDR_DTL             = B.ADDR_DTL        
							 ,MTEL_NO              = B.MTEL_NO         
							 ,TEL_NO               = B.TEL_NO          
							 ,FAX_NO               = B.FAX_NO          
							 ,EML                  = B.EML             
							 ,BIZ_REG_NO           = B.BIZ_REG_NO      
							 ,BUSI_CNDT            = B.BUSI_CNDT       
							 ,BUSI_TP              = B.BUSI_TP         
							 ,TAX_TP               = B.TAX_TP          
							 ,PZIP_CD              = B.PZIP_CD         
							 ,PBASE_ADDR           = B.PBASE_ADDR      
							 ,PADDR_DTL            = B.PADDR_DTL       
							 ,CITY                 = B.CITY            
							 ,STATE                = B.STATE           
							 ,STATE_NM             = B.STATE_NM        
							 ,PMTEL_NO             = B.PMTEL_NO        
							 ,PTEL_NO              = B.PTEL_NO         
							 ,PFAX_NO              = B.PFAX_NO         
							 ,PEML                 = B.PEML            
							 ,PYM_MTHD             = B.PYM_MTHD        
							 ,BILL_MDM_SMS_YN      = B.BILL_MDM_SMS_YN 
							 ,BILL_MDM_GIRO_YN     = B.BILL_MDM_GIRO_YN
							 ,BILL_MDM_EML_YN      = B.BILL_MDM_EML_YN 
							 ,BNK_CD               = B.BNK_CD          
							 ,CDTCD_EXP_DT         = B.CDTCD_EXP_DT    
							 ,USE_ACNT_NM_YN       = B.USE_ACNT_NM_YN  
							 ,ACNT_OWNER_NM        = B.ACNT_OWNER_NM   
							 ,ACNT_NO              = B.ACNT_NO         
							 ,TBR_FLG              = B.TBR_FLG         
							 ,ARRS_NOBILL_YN       = B.ARRS_NOBILL_YN  
							 ,PYM_DT               = B.PYM_DT       
    </insert>
        
