			, Y.SVC_CD AS SVC_CD													-- 서비스코드
			, Y.SVC_GRP AS SVC_GRP													-- 서비스군
			, Y.ATRT_CORP_ID AS ATRT_CORP_ID										-- 유치업체ID
			, Y.ATRT_EMP_ID AS ATRT_EMP_ID											-- 유치사원ID
			, Y.TAX_TP AS TAX_TP													-- 과세구분
			, Y.CUST_TP AS CUST_TP													-- 과세구분
