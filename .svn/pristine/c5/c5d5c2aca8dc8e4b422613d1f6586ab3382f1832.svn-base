					, A.BILL_YYMM
					, A.BILL_CYCL
					, A.BILL_DT
					, A.SO_ID
					, A.BILL_MM_TP
					, A.PYM_ACNT_ID
					, A.CTRT_ID
					, A.CRNCY_CD
					, A.EXRATE
