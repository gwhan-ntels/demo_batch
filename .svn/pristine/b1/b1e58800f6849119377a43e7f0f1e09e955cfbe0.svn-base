package com.ntels.ccbs.batch.common.exception;

public enum BatchError {
	
	EMPTY_SO_ID("SO ID cannot be null! SO ID는 비어있을 수 없습니다.")
	, EMPTY_BILL_YYMM("BILL_YYMM cannot be null! BILL_YYMM은 비어있을 수 없습니다.")
	, EMPTY_BILL_CYCL("BILL_CYCL cannot be null! BILL_CYCL은 비어있을 수 없습니다.")
	, EMPTY_BILL_DT("BILL_DT cannot be null! BILL_DT은 비어있을 수 없습니다.")
	, EMPTY_P_SEQ("P_SEQ cannot be null! P_SEQ은 비어있을 수 없습니다.");
	
	private String message;
	
	BatchError(String message) {
		this.message = message;
	}
	
	public String getMessage() {
		return this.message;
	}
	
}
