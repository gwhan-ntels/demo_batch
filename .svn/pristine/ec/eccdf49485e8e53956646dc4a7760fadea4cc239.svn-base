	public boolean equalsBill(BillInvoice billInvoice) {

		if (billInvoice == null) {
			return false;
		}

		if (getBillSeqNo().equals(billInvoice.getBillSeqNo()) == true && getBillYymm().equals(billInvoice.getBillYymm())
				&& getBillCycl().equals(billInvoice.getBillCycl())
				&& getPymAcntId().equals(billInvoice.getPymAcntId())) {
			return true;
		}

		return false;

	}

	public boolean equalsProd(BillInvoice billInvoice) {

		if (billInvoice == null) {
			return false;
		}

		if (equalsBill(billInvoice) == true && getProdCd().equals(billInvoice.getProdCd()) == true) {
			return true;
		}

		return false;

	}

