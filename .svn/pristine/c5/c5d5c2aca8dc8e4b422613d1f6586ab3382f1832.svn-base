	<update id="updateBillTgtCust">
		UPDATE TBLIV_BILL_TGT_CUST SET
			BILL_MDM_JIRO_YN = 'Y'
			, BILL_MDM_SMS_YN = 'N'
		WHERE A.BILL_YYMM = #{billYymm}
		AND A.BILL_CYCL = '01'
		AND A.SO_ID = #{soId}
		AND A.PYM_ACNT_ID IN 
		(
			SELECT DISTINCT A.PYM_ACNT_ID
			FROM TBLIV_BILL_TGT_CUST A
			WHERE A.BILL_YYMM = #{billYymm}
			AND A.SO_ID = #{soId}
		)
	</update>

