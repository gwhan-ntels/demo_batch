package com.ntels.ccbs.batch.py.entity;

import java.util.ArrayList;
import java.util.List;

public class BatchReceipt {

	private Deposit deposit;
	
	private List<AmbgOcc> ambgOcc;
	
	private List<PrepayOcc> prepayOcc;
	
	private List<Receipt> receipt;
	
	private List<ReceiptDetail> receiptDetail;

	public Deposit getDeposit() {
		return deposit;
	}

	public void setDeposit(Deposit deposit) {
		this.deposit = deposit;
	}

	public List<AmbgOcc> getAmbgOcc() {
		return ambgOcc;
	}

	public void addAmbgOcc(AmbgOcc ambgOcc) {
		if (this.ambgOcc == null) {
			this.ambgOcc = new ArrayList<>();
		}
		
		this.ambgOcc.add(ambgOcc);
	}

	public List<PrepayOcc> getPrepayOcc() {
		return prepayOcc;
	}

	public void addPrepayOcc(PrepayOcc prepayOcc) {
		if (this.prepayOcc == null) {
			this.prepayOcc = new ArrayList<>();
		}
		
		this.prepayOcc.add(prepayOcc);
	}

	public List<Receipt> getReceipt() {
		return receipt;
	}

	public void addReceipt(Receipt receipt) {
		if (this.receipt == null) {
			this.receipt = new ArrayList<>();
		}
		
		this.receipt.add(receipt);
	}

	public List<ReceiptDetail> getReceiptDetail() {
		return receiptDetail;
	}

	public void addReceiptDetail(ReceiptDetail receiptDetail) {
		if (this.receiptDetail == null) {
			this.receiptDetail = new ArrayList<>();
		}
		
		this.receiptDetail.add(receiptDetail);
	}
	
}
